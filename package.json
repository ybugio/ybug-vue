{
  "name": "ybug-vue",
  "version": "1.1.0",
  "type": "module",
  "main": "dist/index.umd.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint src --ext .vue,.js,.jsx,.ts,.tsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.(js|ts|tsx)\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ybugio/ybug-vue.git"
  },
  "keywords": [
    "vue",
    "ybug",
    "ybug.io",
    "feedback",
    "feedback widget"
  ],
  "author": "Radim Hernych",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ybugio/ybug-vue/issues"
  },
  "homepage": "https://github.com/ybugio/ybug-vue#readme",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^18.15.5",
    "@types/jest": "^29.5.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-static-copy": "^0.13.1"
  },
  "peerDependencies": {
    "vue": "^3"
  }
}
